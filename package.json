{
  "name": "component-scraper",
  "version": "1.0.0",
  "description": "CLI tool to create new packages in a monorepo structure",
  "bin": {
    "create-component-package": "./bin/create-component-package.js"
  },
  "files": [
    "bin",
    "_templates"
  ],
  "scripts": {
    "test": "vitest run --silent=false --disable-console-intercept",
    "test:watch": "vitest",
    "create:package": "hygen package new",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "monorepo",
    "generator",
    "package",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.1",
  "dependencies": {
    "enquirer": "^2.3.6",
    "execa": "^9.5.2",
    "hygen": "^6.2.11"
  },
  "devDependencies": {
    "@types/node": "^22",
    "rimraf": "^5.0.5",
    "vitest": "^3",
    "typescript": "^5.8.3"
  }
}
